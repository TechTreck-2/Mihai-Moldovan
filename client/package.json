{
  "name": "techtrek",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build:github": "ng build --configuration github-pages",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:techtrek": "node dist/techtrek/server/server.mjs",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "e2e:headless": "cypress run --headless",
    "e2e:chrome": "cypress run --browser chrome",
    "start:e2e": "concurrently \"npm run start\" \"wait-on http://localhost:4200 && npm run e2e:open\"",
    "screenshots": "node take-screenshots.js",
    "screenshots:update-readme": "node update-readme.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/platform-server": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@angular/ssr": "^17.3.12",
    "@fullcalendar/angular": "^6.1.16",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@popperjs/core": "^2.11.8",
    "angular-calendar": "^0.31.1",
    "bootstrap": "^5.3.2",
    "date-fns": "^4.1.0",
    "express": "^4.18.2",
    "leaflet": "^1.9.4",
    "ng5-slider": "^1.2.6",
    "ol": "^10.4.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.12",
    "@angular/cli": "^17.3.12",
    "@angular/compiler-cli": "^17.3.0",
    "@angular/localize": "^17.3.0",
    "@cypress/angular": "^3.0.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/leaflet": "^1.9.16",
    "@types/node": "^18.18.0",
    "@types/ol": "^7.0.0",
    "concurrently": "^9.2.0",
    "cypress": "^14.5.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "playwright": "^1.54.2",
    "typescript": "~5.4.2",
    "wait-on": "^8.0.3"
  }
}
